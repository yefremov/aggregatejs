{
  "name": "aggregatejs",
  "version": "1.2.1",
  "description": "Aggregate functions returning single result based on groups of data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test",
    "lib": "dist"
  },
  "keywords": [
    "aggregation",
    "average",
    "count",
    "deviation",
    "geometric-mean",
    "harmonic-mean",
    "max",
    "median",
    "min",
    "mode",
    "percentile",
    "quartiles",
    "range",
    "statistics",
    "sum",
    "typescript",
    "variance"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "ts-node node_modules/tape/bin/tape test/*.ts | tap-spec",
    "test-cov": "nyc npm test",
    "report-cov": "mkdir -p coverage && nyc report --reporter=text-lcov > coverage/lcov.info",
    "perf": "ts-node test/performance/all.bench.ts",
    "perf:basic": "ts-node test/performance/basic.bench.ts",
    "perf:positional": "ts-node test/performance/positional.bench.ts",
    "perf:specialized": "ts-node test/performance/specialized.bench.ts",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yefremov/aggregatejs.git"
  },
  "author": "Anton Yefremov <anton.yefremov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yefremov/aggregatejs/issues"
  },
  "homepage": "https://github.com/yefremov/aggregatejs#readme",
  "devDependencies": {
    "@types/benchmark": "^2.1.5",
    "@types/node": "^20.19.30",
    "@types/tape": "^5.8.1",
    "benchmark": "^2.1.4",
    "nyc": "^17.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3"
  }
}
